{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "stage": "npm run staging",
        "staging": "cross-env NODE_ENV=staging node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "startDev": "concurrently \"npm run watch\" \"php artisan serve\""
    },
    "devDependencies": {
        "axios": "^0.19",
        "bootstrap": "^4.1.0",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^4.1.4",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.6.14",
        "vue-avatar-component": "^1.3.1",
        "vue-template-compiler": "^2.6.14"
    },
    "dependencies": {
        "@inotom/vue-curve-text": "^2.0.0",
        "apexcharts": "^3.44.0",
        "bootstrap-icons": "^1.10.5",
        "concurrently": "^8.2.1",
        "debounce": "^1.2.1",
        "gsap": "npm:@gsap/business@^3.12.2",
        "recaptcha-v3": "^1.10.0",
        "vue-apexcharts": "^1.6.2",
        "vue-country-flag": "^2.3.2",
        "vue-gtag": "^1.16.1",
        "vue-i18n": "^8.28.2",
        "vue-recaptcha-v3": "^1.9.0",
        "vue-router": "^3.5.3",
        "vuetify": "^2.5.12"
    }
}
